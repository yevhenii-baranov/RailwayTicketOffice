      </Compile>
    </ItemGroup>

    <ItemGroup>
      <EmbeddedResource Update="UI\MainMenu.resx">
        <DependentUpon>MainMenu.cs</DependentUpon>
      </EmbeddedResource>
    </ItemGroup>

</Project>
