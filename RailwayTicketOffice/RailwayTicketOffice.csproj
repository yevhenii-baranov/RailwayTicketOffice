<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

    <PropertyGroup>
        <TargetFramework>netcoreapp3.0</TargetFramework>
        <ApplicationIcon />
        <OutputType>WinExe</OutputType>
        <StartupObject />
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Microsoft.EntityFrameworkCore" Version="2.2.0" />
        <PackageReference Include="Microsoft.EntityFrameworkCore.Design" Version="2.2.0" />
        <PackageReference Include="MySql.Data.EntityFrameworkCore" Version="8.0.13" />
    </ItemGroup>

    <ItemGroup>
      <Folder Include="Properties\" />
    </ItemGroup>

    <ItemGroup>
      <Compile Update="UI\PassportDataForm.cs">
        <SubType>Form</SubType>
      </Compile>
      <Compile Update="UI\PassportDataForm.Designer.cs">
        <DependentUpon>PassportDataForm.cs</DependentUpon>
      </Compile>
      <Compile Update="UI\TicketChooseForm.cs">
        <SubType>Form</SubType>
      </Compile>
      <Compile Update="UI\TicketChooseForm.Designer.cs">
        <DependentUpon>TicketChooseForm.cs</DependentUpon>
      </Compile>
      <Compile Update="UI\TicketInfo.cs">
        <SubType>Form</SubType>
      </Compile>
      <Compile Update="UI\TicketInfo.Designer.cs">
        <DependentUpon>TicketInfo.cs</DependentUpon>
      </Compile>
      <Compile Update="UI\LoginForm.cs">
        <SubType>Form</SubType>
      </Compile>
      <Compile Update="UI\LoginForm.Designer.cs">
        <DependentUpon>LoginForm.cs</DependentUpon>
      </Compile>
      <Compile Update="UI\RegisterForm.cs">
        <SubType>Form</SubType>
      </Compile>
      <Compile Update="UI\RegisterForm.Designer.cs">
        <DependentUpon>RegisterForm.cs</DependentUpon>
      </Compile>
      <Compile Update="UI\Schedule.cs">
        <SubType>Form</SubType>
      </Compile>
      <Compile Update="UI\Schedule.Designer.cs">
        <DependentUpon>Schedule.cs</DependentUpon>
      </Compile>
    </ItemGroup>

    <ItemGroup>
      <EmbeddedResource Update="UI\PassportDataForm.resx">
        <DependentUpon>PassportDataForm.cs</DependentUpon>
      </EmbeddedResource>
      <EmbeddedResource Update="UI\TicketChooseForm.resx">
        <DependentUpon>TicketChooseForm.cs</DependentUpon>
      </EmbeddedResource>
      <EmbeddedResource Update="UI\TicketInfo.resx">
        <DependentUpon>TicketInfo.cs</DependentUpon>
      </EmbeddedResource>
      <EmbeddedResource Update="UI\LoginForm.resx">
        <DependentUpon>LoginForm.cs</DependentUpon>
      </EmbeddedResource>
      <EmbeddedResource Update="UI\RegisterForm.resx">
        <DependentUpon>RegisterForm.cs</DependentUpon>
      </EmbeddedResource>
      <EmbeddedResource Update="UI\Schedule.resx">
        <DependentUpon>Schedule.cs</DependentUpon>
      </EmbeddedResource>
    </ItemGroup>

</Project>
