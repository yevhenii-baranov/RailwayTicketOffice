        <PackageReference Include="MySql.Data.EntityFrameworkCore" Version="8.0.13" />
    </ItemGroup>

    <ItemGroup>
      <Folder Include="Properties\" />
    </ItemGroup>

    <ItemGroup>
      <Compile Update="UI\MainMenu.cs">
        <SubType>Form</SubType>
      </Compile>
      <Compile Update="UI\MainMenu.Designer.cs">
